@model List<ProductCategory.Models.Domain.Category>

   
@{

}


<h1>Category</h1>

<table class="table">
    <thead>
        <tr>
            <th>Category Id</th>
            <th>Category Name</th>
            <th>Category Update</th>
        </tr>
    </thead>
    <tbody class="mt-5">
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.CategoryId</td>
                <td>@category.CategoryName</td>
                <td><a href="AddCategory/View/@category.CategoryId">Update</a></td>
                @*<td><a href="AddCategory/DeleteCategory/@category.CategoryId">Delete</a></td>*@
                
            </tr>
        }
    </tbody>
</table>
   
